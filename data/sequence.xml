<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sale_contract_sequence" model="ir.sequence">
            <field name="name">sale.contract.seq</field>
            <field name="code">sale.contract</field>
            <field name="prefix">SC/</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_sale_return" model="ir.sequence">
            <field name="name">Sales Return</field>
            <field name="code">sale.return</field>
            <field name="prefix">SR</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>